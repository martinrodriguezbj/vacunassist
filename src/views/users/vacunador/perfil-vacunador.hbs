<h1></h1>
<h2>
    <center>Vacunador</center>
</h2>
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Apellido</th>
            <th scope="col">DNI</th>
            <th scope="col">Email</th>
        </tr>
    </thead>
    <tbody>
        {{#each vacunador}}
        <tr>
            <th>{{name}}</th>
            <th>{{surname}}</th>
            <th>{{dni}}</th>
            <th>{{email}}</th>
        </tr>
        {{/each}}
    </tbody>
</table>